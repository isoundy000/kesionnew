<block wx:if="{{show}}"><view class="exam-app"><view class="my-exam-tab"><view style="display:flex;flex:1;"><view class="my-exam-tab__topn">{{allData.tk_num+"题"}}</view><view class="my-exam-tab__topn">{{allData.paper_num+"套试卷"}}</view><view class="my-exam-tab__topn">{{allData.knowledge_num+"个知识点"}}</view></view><view data-event-opts="{{[['tap',[['openSelect']]]]}}" class="my-exam-tab__topleft" bindtap="__e"><text>{{classes}}</text><image src="{{baseUrl+'/static/images/choose-cade/choose-Cades.png'}}"></image></view></view><view class="examMain"><view class="middlep"><block wx:for="{{moduleOptions}}" wx:for-item="item" wx:for-index="__i0__" wx:key="module_id"><view style="width:calc(100% / 3);text-align:center;"><view data-event-opts="{{[['tap',[['url',['$0'],[[['moduleOptions','module_id',item.module_id,'module_name']]]]]]]}}" bindtap="__e"><view class="middlep-img" align="center"><image src="{{baseUrl+item.src}}"></image></view><view class="middlep_text" align="center">{{item.module_name}}</view></view></view></block></view><block wx:if="{{uchart}}"><view class="myexam-chart"><view style="font-size:14px;font-weight:500;color:#333;height:36px;line-height:36px;padding-left:10px;">考试学习情况</view><ucharts vue-id="1" opts="{{opts}}" canvas-id="canvasLineStudy" chartType="line" cWidth="{{cWidth}}" cHeight="{{cHeight}}" bind:__l="__l"></ucharts></view></block></view><view class="examBottom"><view class="learn-card"><view class="learn-card__head" style="width:calc(100% / 3);">连续学习</view><view class="learn-card__head" style="width:calc(100% / 3);">学习时间</view><view class="learn-card__head" style="width:calc(100% / 3);">正确率</view><view class="learn-card__siz" style="width:calc(100% / 3);"><text class="learn-card__span">{{allData.total_continedotkday}}</text>天</view><view class="learn-card__siz" style="width:calc(100% / 3);"><text class="learn-card__span">{{allData.total_dotktime}}</text>分钟</view><view class="learn-card__siz" style="width:calc(100% / 3);"><text class="learn-card__span">{{allData.zhunquelv}}</text>%</view></view><view class="examBottom-position"><view class="qpIcon"><view class="qpIcon-box"><image src="{{baseUrl+'/static/images/exam/qp1.png'}}"></image><text>{{allData.user_knowledge_num+"/"+allData.knowledge_num+"知识点"}}</text></view><view class="qpIcon-box"><image src="{{baseUrl+'/static/images/exam/qp2.png'}}"></image><text>{{allData.user_paper_num+"/"+allData.paper_num+"试卷"}}</text></view><view class="qpIcon-box"><image src="{{baseUrl+'/static/images/exam/qp3.png'}}"></image><text>{{allData.do_num+"/"+allData.tk_num+"题"}}</text></view></view><view class="progress-position"><view class="progress-bar1" style="{{'width:'+(knowledgePro+'%')+';'+('z-index:'+(knowledgeNum)+';')}}"></view><view class="progress-bar2" style="{{'width:'+(examPro+'%')+';'+('z-index:'+(examNum)+';')}}"></view><view class="progress-bar3" style="{{'width:'+(bankPro+'%')+';'+('z-index:'+(bankNum)+';')}}"></view></view></view></view></view></block>